{
	"name": "yohira",
	"version": "0.0.0-alpha.8",
	"description": "Yohira is an experimental project to port ASP.NET Core to TypeScript.",
	"main": "dist/index.js",
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^3.4.0",
		"@types/node": "^18.11.11",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.43.0",
		"concurrently": "^7.6.0",
		"conditional-type-checks": "^1.0.6",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"json5": "^2.2.3",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"tsc-alias": "^1.7.1",
		"typescript": "^5.0.0-beta",
		"vitest": "^0.26.2"
	},
	"scripts": {
		"clean": "rimraf ./dist",
		"build": "npm run clean && tsc && tsc-alias",
		"build:watch": "npm run clean && tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
		"start": "node dist/main.js",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ycanardeau/yohira.git"
	},
	"keywords": [
		"aspnetcore",
		"nodejs",
		"typescript"
	],
	"author": "Aigamo",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ycanardeau/yohira/issues"
	},
	"homepage": "https://github.com/ycanardeau/yohira#readme",
	"dependencies": {
		"reflect-metadata": "^0.1.13"
	}
}
