{
	"singleQuote": true,
	"trailingComma": "all",
	"importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderParserPlugins": [
		"jsx",
		"typescript",
		"importOrderParserPlugins",
		"classProperties",
		"decorators-legacy",
		"importAssertions",
		"explicitResourceManagement"
	],
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
