{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022"],
		"module": "commonjs",
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"baseUrl": "./"
	}
}
