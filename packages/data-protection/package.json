{
	"name": "@yohira/data-protection",
	"version": "0.0.0-alpha.46",
	"description": "Yohira is an experimental project to port ASP.NET Core to TypeScript.",
	"main": "dist/index.js",
	"dependencies": {
		"@yohira/base": "file:../base",
		"@yohira/cryptography": "file:../cryptography",
		"@yohira/data-protection.abstractions": "file:../data-protection.abstractions",
		"@yohira/extensions.dependency-injection.abstractions": "file:../extensions.dependency-injection.abstractions",
		"@yohira/extensions.logging.abstractions": "file:../extensions.logging.abstractions",
		"@yohira/extensions.options": "file:../extensions.options",
		"@yohira/xml": "file:../xml"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/node": "^18.19.5",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"@vitest/coverage-c8": "^0.29.8",
		"concurrently": "^7.6.0",
		"conditional-type-checks": "^1.0.6",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"json5": "^2.2.3",
		"prettier": "^3.2.5",
		"rimraf": "^3.0.2",
		"tsc-alias": "^1.7.1",
		"typescript": "^5.4.5",
		"vitest": "^0.29.8"
	},
	"scripts": {
		"clean": "rimraf ./dist",
